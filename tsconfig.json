{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitAny": false,
    "allowJs": true,
    "lib": [
      "es2021",
      "DOM"
    ],
    "module": "commonjs",
    "target": "es2021",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}